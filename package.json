{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:optimized": "vite build --mode production --minify",
        "build:analyze": "vite build --mode production --analyze",
        "dev": "vite",
        "dev:fast": "vite --force",
        "preview": "vite preview",
        "optimize": "npm run build:optimized && npm run cache:clear",
        "cache:clear": "php artisan cache:clear && php artisan config:clear && php artisan route:clear && php artisan view:clear",
        "performance:test": "php artisan performance:test",
        "database:optimize": "php artisan migrate && php artisan db:seed --class=PerformanceSeeder"
    },
    "dependencies": {
        "axios": "^1.8.2",
        "chart.js": "^2.9.4",
        "leaflet": "^1.9.4",
        "select2": "^4.1.0-rc.0"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.2.0",
        "tailwindcss": "^4.0.0",
        "vite": "^6.2.4",
        "vite-bundle-analyzer": "^0.7.0",
        "terser": "^5.24.0",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "mini-css-extract-plugin": "^2.7.6",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^10.0.0"
    }
